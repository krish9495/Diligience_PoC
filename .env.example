# Environment Configuration for Diligence_PoC
# Copy this file to .env and replace placeholder values with your actual credentials

# Gemini Configuration
LLM_PROVIDER="gemini"
LLM_MODEL="gemini/gemini-2.5-flash"
LLM_API_KEY="your-gemini-api-key-here"

# Embedding Configuration
EMBEDDING_PROVIDER="gemini"
EMBEDDING_MODEL="gemini/text-embedding-004"
EMBEDDING_API_KEY="your-embedding-api-key-here"
EMBEDDING_DIMENSIONS="768"

# Google API Configuration
GOOGLE_API_KEY="your-google-api-key-here"

# Database Configuration (used by cognee_poc.py)
MIGRATION_DB_PROVIDER=sqlite
MIGRATION_DB_PATH=demo_files
MIGRATION_DB_NAME=alpha_fund_data.db
